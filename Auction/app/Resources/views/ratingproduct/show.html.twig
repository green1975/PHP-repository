{% extends 'base.html.twig' %}

{% block body %}
    <h1>Ratingproduct</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ratingproduct.id }}</td>
            </tr>
            <tr>
                <th>Rating</th>
                <td>{{ ratingproduct.rating }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ ratingproduct.comment }}</td>
            </tr>
            <tr>
                <th>Userid</th>
                <td>{{ ratingproduct.userId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('ratingproduct_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('ratingproduct_edit', { 'id': ratingproduct.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
